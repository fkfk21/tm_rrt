#! /bin/sh
ECLIPSEDIR="${ECLIPSEDIR:-/home/hargalaten/catkin_ws/src/seed/eclipseclp}"
LD_LIBRARY_PATH="$ECLIPSEDIR/lib/x86_64_linux:$LD_LIBRARY_PATH:/usr/local/lib"
TCL_LIBRARY="${TCL_LIBRARY:-/usr/share/tcltk/tcl8.5}"
TK_LIBRARY="${TK_LIBRARY:-/usr/share/tcltk/tk8.5}"
DAVINCIHOME="${DAVINCIHOME:-$ECLIPSEDIR/daVinci/x86_64_linux}"
JRE_HOME="${JRE_HOME:-}"
export ECLIPSEDIR TCL_LIBRARY TK_LIBRARY  LD_LIBRARY_PATH DAVINCIHOME JRE_HOME
exec "/usr/bin/wish" "/home/hargalaten/catkin_ws/src/seed/eclipseclp/lib_tcl/tkeclipse.tcl" -- "$@"
